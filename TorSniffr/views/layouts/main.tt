:block}.adverts-top{margin:0 0 20px 0;padding-bottom:10px;border-bottom:1px solid #7D4698;}.red-warning__small{font-size:small;color:#c03;vertical-align:top}@media (max-width:479px){.adverts{margin:1em auto;text-align:center;overflow:hidden}.adverts a{display:block;margin:1em auto}.adverts img{display:block;width:100%;height:100%}#form-container input[type=submit],.half-width{width:100%}.half-width{margin:1em 0}#form-container input[type=submit]{margin-top:1em}#form-container input[type=text]{width:100%;margin-top:.5em}#form-container label{width:100%;display:inline-block}.intro-text{clear:both;padding-top:1em}

